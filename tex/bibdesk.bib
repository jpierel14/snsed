	Author = {{Perley}, D.~A. and {Metzger}, B.~D. and {Granot}, J. and {Butler}, N.~R. and {Sakamoto}, T. and {Ramirez-Ruiz}, E. and {Levan}, A.~J. and {Bloom}, J.~S. and {Miller}, A.~A. and {Bunker}, A. and {Chen}, H.-W. and {Filippenko}, A.~V. and {Gehrels}, N. and {Glazebrook}, K. and {Hall}, P.~B. and {Hurley}, K.~C. and {Kocevski}, D. and {Li}, W. and {Lopez}, S. and {Norris}, J. and {Piro}, A.~L. and {Poznanski}, D. and {Prochaska}, J.~X. and {Quataert}, E. and {Tanvir}, N.},
