	Keywords = {Cosmology: Cosmological Parameters; Galaxies: High-Redshift; Stars: Supernovae: General},
	Pages = {359--368},
	Read = {Yes},
	Title = {Timescale Stretch Parameterization of Type Ia Supernova B-Band Light Curves},
	Ty = {JOUR},
