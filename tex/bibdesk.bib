	Adsurl = {http://adsabs.harvard.edu/abs/2013ApJ...766...70S},
	Archiveprefix = {arXiv},
	Arxivurl = {http://arXiv.org/abs/1208.6010},
	Author = {{Suyu}, S.~H. and {Auger}, M.~W. and {Hilbert}, S. and {Marshall}, P.~J. and {Tewes}, M. and {Treu}, T. and {Fassnacht}, C.~D. and {Koopmans}, L.~V.~E. and {Sluse}, D. and {Blandford}, R.~D. and {Courbin}, F. and {Meylan}, G.},
	Date-Added = {2014-03-28 20:43:12 +0000},
