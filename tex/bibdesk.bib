@article{Turner:1997,
	Abstract = {The inflationary prediction of a flat Universe is at odds with current determinations of the matter density (OmegaM~=0.2-0.4). This dilemma can be resolved if a smooth component contributes the remaining energy density (OmegaX=1-OmegaM). We parametrize the smooth component by its equation of state, pX=wrhoX, and show that XCDM with w~=-0.6, OmegaM~=0.3, and h~=0.7 is the best fit to all present cosmological data. Together, the position of the peak in the CMB angular power spectrum and the type Ia supernova magnitude-redshift diagram provide a crucial test of XCDM.},
