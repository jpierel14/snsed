	Journal = {\apjl},
	Keywords = {Galaxies: Statistics, Cosmology: Large-Scale Structure of Universe},
	Month = jan,
	Pages = {L171-L174},
	Title = {{Cosmic Variance in the Great Observatories Origins Deep Survey}},
