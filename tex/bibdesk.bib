	Eprint = {0710.3587},
	Journal = {\mnras},
	Keywords = {galaxies: evolution , galaxies: high-redshift , galaxies: starburst , cosmology: observations , gamma-rays: bursts},
	Month = aug,
	Pages = {1487-1500},
	Title = {{Star formation history up to z = 7.4: implications for gamma-ray bursts and cosmic metallicity evolution}},
	Url = {http://adsabs.harvard.edu/abs/2008MNRAS.388.1487L},
	Volume = 388,
	Year = 2008,
