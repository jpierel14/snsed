	Eprint = {1409.0307},
	Journal = {\mnras},
	Keywords = {galaxies: abundances, galaxies: clusters: general, galaxies: clusters: intracluster medium},
	Month = nov,
	Pages = {3581-3591},
	Title = {{Chemical evolution on the scale of clusters of galaxies: a conundrum?}},
	Volume = 444,
	Year = 2014,
