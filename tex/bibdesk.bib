@article{Coles:2008,
	Abstract = {This paper examines free-form modeling of gravitational lenses using Bayesian ensembles of pixelated mass maps. The priors and algorithms from previous work are clarified and significant technical improvements are made. Lens reconstruction and Hubble time recovery are tested using mock data from simple analytic models and recent galaxy formation simulations. Finally, using published data, the Hubble time is inferred through the simultaneous reconstruction of 11 time delay lenses. The result is H-10=13.7+1.8-1.0 Gyr (H0=71+6-8 km s-1 Mpc-1).},
