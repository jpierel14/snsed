	Date-Modified = {2009-10-23 15:20:08 -1000},
	Doi = {10.1016/S0370-2693(00)00631-6},
	Eprint = {arXiv:hep-th/0002190},
	Journal = {Physics Letters B},
	Month = jun,
	Pages = {112-118},
	Title = {{Metastable gravitons and infinite volume extra dimensions}},
	Url = {http://adsabs.harvard.edu/abs/2000PhLB..484..112D},
	Volume = 484,
	Year = 2000,
	Bdsk-Url-1 = {http://dx.doi.org/10.1016/S0370-2693(00)00631-6},
