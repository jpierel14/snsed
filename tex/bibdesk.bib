	Title = {Optical and Infrared Spectroscopy of SN 1999ee and SN 1999ex},
	Ty = {JOUR},
	Url = {http://adsabs.harvard.edu/abs/2002AJ....124..417H},
	Volume = {124},
	Year = {2002},
