	Journal = {\aj},
	Keywords = {Stars: Supernovae: Individual: Alphanumeric: SN 1999ee; Stars: Supernovae: Individual: Alphanumeric: SN 1999ex; Techniques: Photometric},
	Pages = {2100--2117},
	Read = {Yes},
	Title = {Optical Photometry of the Type Ia Supernova 1999ee and the Type Ib/c Supernova 1999ex in IC 5179},
