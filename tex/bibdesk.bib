	Read = {Yes},
	Title = {Applying Bayesian Model Averaging to mechanistic models: An example and comparison of methods},
	Url = {http://www.sciencedirect.com/science/article/B6VHC-4RM89FV-1/2/69fc95b900bace13f57b3feadd3004c2},
