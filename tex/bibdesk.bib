	Keywords = {STARS: BINARIES: CLOSE, STARS: BINARIES: SYMBIOTIC, STARS: EVOLUTION, STARS: MASS LOSS, STARS: SUPERNOVAE: GENERAL},
	Month = sep,
	Pages = {487-503},
	Title = {{A Wide Symbiotic Channel to Type IA Supernovae}},
	Url = {http://adsabs.harvard.edu/abs/1999ApJ...522..487H},
