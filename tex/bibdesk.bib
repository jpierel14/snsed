	Journal = {\apj},
	Keywords = {Cosmology: Cosmological Parameters; Cosmology: Distance Scale; Stars: Supernovae: General},
	Pages = {944--970},
	Read = {Yes},
	Title = {Multicolor Light Curves of Type Ia Supernovae on the Color-Magnitude Diagram: A Novel Step toward More Precise Distance and Extinction Estimates},
	Ty = {JOUR},
	Url = {http://adsabs.harvard.edu/abs/2003ApJ...590..944W},
