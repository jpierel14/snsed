	Keywords = {TECHNIQUES: IMAGE PROCESSING; ASTRONOMICAL DATA BASES: MISCELLANEOUS; SUPERNOVAE: GENERAL; GALAXIES: STARBURST; INFRARED: GALAXIES},
	Pages = {325--342},
	Read = {Yes},
	Title = {Supernovae in the nuclear regions of starburst galaxies},
