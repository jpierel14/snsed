	Eprint = {1508.03141},
	Journal = {ArXiv e-prints},
	Keywords = {Astrophysics - Solar and Stellar Astrophysics, Astrophysics - High Energy Astrophysical Phenomena},
	Month = aug,
	Primaryclass = {astro-ph.SR},
	Title = {{Growing White Dwarfs to the Chandrasekhar Limit: The Parameter Space of the Single Degenerate SNIa Channel}},
