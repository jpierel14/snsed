	Journal = {\aap},
	Keywords = {gravitational lensing: strong, cosmological parameters},
	Month = apr,
	Pages = {A103},
	Title = {{Source-position transformation: an approximate invariance in strong gravitational lensing}},
	Volume = 564,
	Year = 2014,
