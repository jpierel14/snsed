	Journal = {\apj},
	Keywords = {Cosmology: Observations; Cosmology: Distance Scale; Stars: Supernovae: General},
	Pages = {50--69},
	Read = {Yes},
	Title = {Nonlinear Decline-Rate Dependence and Intrinsic Variation of Type Ia Supernova Luminosities},
