	Keywords = {Algorithms, Dark Matter, Gamma Ray Bursts, Gravitational Lenses, Light Transmission, Magnification, Physical Optics, Wavelengths, Fourier Transformation, Mass Distribution, Spectral Resolution, Wave Diffraction, White Dwarf Stars},
	Month = mar,
	Pages = {67-75},
	Title = {{Femtolensing: Beyond the semiclassical approximation}},
