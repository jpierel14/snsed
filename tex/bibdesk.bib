	Month = jul,
	Pages = {876-886},
	Read = {Yes},
	Title = {{Epochs of maximum light and bolometric light curves of type Ia supernovae}},
	Url = {http://adsabs.harvard.edu/abs/2000A%26A...359..876C},
	Volume = 359,
