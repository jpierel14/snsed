	Doi = {10.1046/j.1365-8711.2002.05274.x},
	Eprint = {astro-ph/0109089},
	Journal = {\mnras},
	Keywords = {supernovae: general, galaxies: clusters: general},
	Month = may,
	Pages = {37-48},
