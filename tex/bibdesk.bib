	Doi = {10.1086/522030},
	Eprint = {arXiv:astro-ph/0701912},
	Journal = {\apjl},
	Keywords = {Cosmology: Cosmological Parameters, Cosmology: Observations, Stars: Supernovae: General, Surveys},
	Month = sep,
	Pages = {L37-L40},
	Title = {{Predicted and Observed Evolution in the Mean Properties of Type Ia Supernovae with Redshift}},
