	Author = {{Tovmassian}, G.~H. and {Napiwotzki}, R. and {Richer}, M.~G. and {Stasi{\'n}ska}, G. and {Fullerton}, A.~W. and {Rauch}, T.},
	Date-Added = {2009-10-21 10:54:07 -1000},
