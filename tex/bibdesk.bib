	Keywords = {dust, extinction},
	Month = jul,
	Pages = {1209-1222},
	Primaryclass = {astro-ph.GA},
	Title = {{An Analysis of the Shapes of Interstellar Extinction Curves. VI. The Near-IR Extinction Law}},
