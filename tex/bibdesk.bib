	Eprint = {arXiv:astro-ph/0305114},
	Journal = {\apjl},
	Keywords = {Galaxies: Abundances, Galaxies: Evolution, Nuclear Reactions, Nucleosynthesis, Abundances, Stars: Abundances, Stars: Supernovae: General},
	Month = jun,
	Pages = {L83-L86},
	Read = {Yes},
