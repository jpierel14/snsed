	Doi = {10.1086/498294},
	Eprint = {astro-ph/0505110},
	Journal = {\apj},
	Keywords = {Cosmology: Theory, Cosmology: Early Universe, Galaxies: Evolution, Galaxies: High-Redshift, Stars: Supernovae: General},
	Month = jan,
	Pages = {80-90},
	Title = {{The Redshift Distribution of Distant Supernovae and Its Use in Probing Reionization}},
