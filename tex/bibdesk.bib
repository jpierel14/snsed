	Doi = {10.1086/424795},
	Eprint = {arXiv:astro-ph/0407518},
	Journal = {\apj},
	Keywords = {Stars: Binaries: Close, Galaxy: Halo, planetary nebulae: individual (PN G135.9+55.9, SBS 1150+599A, Ultraviolet: ISM},
	Month = nov,
	Pages = {485-497},
	Title = {{A Close Binary Nucleus in the MOST Oxygen-Poor Planetary Nebula PN G135.9+55.9}},
	Url = {http://adsabs.harvard.edu/abs/2004ApJ...616..485T},
	Volume = 616,
	Year = 2004,
