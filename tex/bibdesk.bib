	Author = {{Berger}, E. and {Leibler}, C.~N. and {Chornock}, R. and {Rest}, A. and {Foley}, R.~J. and {Soderberg}, A.~M. and {Price}, P.~A. and {Burgett}, W.~S. and {Chambers}, K.~C. and {Flewelling}, H. and {Huber}, M.~E. and {Magnier}, E.~A. and {Metcalfe}, N. and {Stubbs}, C.~W. and {Tonry}, J.~L.},
