	Date-Modified = {2009-07-31 12:09:59 -1000},
	Read = {Yes},
	School = {University of Hawai'i},
	Title = {{Type Ia supernovae at high redshift}},
	Url = {http://adsabs.harvard.edu/abs/2004PhDT.........8B},
	Year = 2004,
