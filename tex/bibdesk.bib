@article{Sollerman:2000,
	Abstract = {
We present observations of the peculiar supernova SN 1998bw, which was
probably associated with GRB 980425. The photometric and spectroscopic
evolution is monitored up to 500 days past explosion. We also present
modeling based on spherically symmetric, massive progenitor models and
