	Eprint = {astro-ph/9712065},
	Journal = {\aap},
	Month = mar,
	Pages = {L69-L72},
	Title = {{Cosmic star formation and Type Ia/II supernova rates at high Z}},
	Volume = 331,
	Year = 1998,
	Bdsk-File-1 = {},
