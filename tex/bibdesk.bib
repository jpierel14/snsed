	Date-Modified = {2009-11-18 10:26:23 -1000},
	Eprint = {0804.0909},
	Journal = {\pasj},
	Keywords = {cosmology: observations, galaxies: evolution, stars: supernovae: general},
	Month = dec,
	Pages = {1327-},
	Title = {{Delay Time Distribution Measurement of Type Ia Supernovae by the Subaru/XMM-Newton Deep Survey and Implications for the Progenitor}},
	Url = {http://adsabs.harvard.edu/abs/2008PASJ...60.1327T},
