	Journal = {\mnras},
	Keywords = {stars: activity, stars: flare, starspots},
	Month = jul,
	Pages = {3420-3429},
	Title = {{Kepler observations of flaring in A-F type stars}},
	Volume = 423,
	Year = 2012,
	Bdsk-File-1 = {},
	Bdsk-Url-1 = {http://dx.doi.org/10.1111/j.1365-2966.2012.21135.x},
