	Doi = {10.1103/PhysRevD.80.123525},
	Eprint = {0910.2492},
	Journal = {\prd},
	Keywords = {Observational cosmology, Dark energy, Mathematical and relativistic aspects of cosmology},
	Month = dec,
	Number = 12,
	Pages = {123525-+},
	Title = {{Distance measurements from supernovae and dark energy constraints}},
	Url = {http://adsabs.harvard.edu/abs/2009PhRvD..80l3525W},
