	Keywords = {gamma rays: diffuse background, gamma rays: stars, nuclear reactions, nucleosynthesis, abundances, stars: statistics, supernovae: general},
	Month = nov,
	Pages = {329-341},
	Primaryclass = {astro-ph.CO},
