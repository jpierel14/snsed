	Eprint = {arXiv:astro-ph/0510315},
	Journal = {\mnras},
	Keywords = {supernovae: general: white dwarfs: galaxies: abundances, supernovae: general, white dwarfs, galaxies: abundances},
	Month = aug,
	Pages = {773-783},
	Read = {Yes},
	Title = {{Two populations of progenitors for Type Ia supernovae?}},
	Url = {http://adsabs.harvard.edu/abs/2006MNRAS.370..773M},
	Volume = 370,
	Year = 2006,
