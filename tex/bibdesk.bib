	Archiveprefix = {arXiv},
	Author = {{Poznanski}, D. and {Maoz}, D. and {Yasuda}, N. and {Foley}, R.~J. and {Doi}, M. and {Filippenko}, A.~V. and {Fukugita}, M. and {Gal-Yam}, A. and {Jannuzi}, B.~T. and {Morokuma}, T. and {Oda}, T. and {Schweiker}, H. and {Sharon}, K. and {Silverman}, J.~M. and {Totani}, T.},
	Date-Added = {2009-05-14 16:19:40 -1000},
	Date-Modified = {2009-05-14 16:19:44 -1000},
	Doi = {10.1111/j.1365-2966.2007.12424.x},
	Eprint = {0707.0393},
	Journal = {\mnras},
	Keywords = {surveys, supernovae: general, cosmology: miscellaneous, cosmology: observations},
	Month = dec,
	Pages = {1169-1186},
