	Doi = {10.1086/430433},
	Eprint = {arXiv:astro-ph/0504087},
	Journal = {\apj},
	Keywords = {Galaxies: Active, Stars: Supernovae: General},
	Month = aug,
	Pages = {750-756},
	Read = {Yes},
	Title = {{Why Are Radio Galaxies Prolific Producers of Type Ia Supernovae?}},
