	Doi = {10.1088/0004-637X/741/2/82},
	Eid = {82},
	Eprint = {1011.4322},
	Journal = {\apj},
	Keywords = {binaries: close, celestial mechanics, gravitational waves, stars: kinematics and dynamics, stars: neutron, supernovae: general, white dwarfs},
	Month = nov,
	Pages = {82},
	Primaryclass = {astro-ph.HE},
	Title = {{Accelerating Compact Object Mergers in Triple Systems with the Kozai Resonance: A Mechanism for ''Prompt'' Type Ia Supernovae, Gamma-Ray Bursts, and Other Exotica}},
