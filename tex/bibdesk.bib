	Journal = {\apjl},
	Keywords = {Stars: Binaries: Close, Galaxies: Evolution, Stars: Winds, Outflows, Stars: Supernovae: General},
	Month = aug,
	Pages = {L127-L130},
	Title = {{The Delay-Time Distribution of Type Ia Supernovae and the Single-Degenerate Model}},
