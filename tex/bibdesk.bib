	Keywords = {Cosmology: Observations; Cosmology: Distance Scale; Galaxies: Distances and Redshifts; Stars: Supernovae: General},
	Pages = {579--607},
	Read = {Yes},
	Title = {Cepheid Calibrations from the Hubble Space Telescope of the Luminosity of Two Recent Type Ia Supernovae and a Redetermination of the Hubble Constant},
	Ty = {JOUR},
