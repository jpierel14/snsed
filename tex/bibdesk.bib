	Eprint = {1105.2860},
	Journal = {\aap},
	Keywords = {white dwarfs, supernovae: general},
	Month = jul,
	Pages = {A94+},
	Primaryclass = {astro-ph.SR},
	Title = {{The correlation between C/O ratio, metallicity, and the initial WD mass for SNe Ia}},
