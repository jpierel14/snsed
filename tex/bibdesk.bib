	Archiveprefix = {arXiv},
	Author = {{Tovmassian}, G. and {Yungelson}, L. and {Rauch}, T. and {Suleimanov}, V. and {Napiwotzki}, R. and {Stasi{\'n}ska}, G. and {Tomsick}, J. and {Wilms}, J. and {Morisset}, C. and {Pe{\~n}a}, M. and {Richer}, M.~G.},
