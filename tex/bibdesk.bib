	Journal = {\aap},
	Keywords = {stars: binaries: close, stars: supernovae: general, galaxies: evolution, galaxies: intergalactic medium, stars: white dwarfs},
	Month = oct,
	Pages = {1055-1078},
	Read = {Yes},
	Title = {{The rates of type Ia supernovae. I. Analytical formulations}},
