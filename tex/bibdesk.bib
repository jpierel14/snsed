	Eprint = {arXiv:astro-ph/0304382},
	Journal = {\pasp},
	Keywords = {Galaxies: Luminosity Function, Mass Function, Invited Reviews},
	Month = jul,
	Pages = {763-795},
	Title = {{Galactic Stellar and Substellar Initial Mass Function}},
	Volume = 115,
