	Date-Modified = {2009-12-05 04:37:17 -1000},
	Doi = {10.1088/1367-2630/8/9/195},
	Eprint = {arXiv:astro-ph/0609489},
	Journal = {New Journal of Physics},
	Month = sep,
	Pages = {195-+},
	Title = {{GRBs as cosmological probes - cosmic chemical evolution}},
	Url = {http://adsabs.harvard.edu/abs/2006NJPh....8..195S},
	Volume = 8,
	Year = 2006,
	Bdsk-Url-1 = {http://dx.doi.org/10.1088/1367-2630/8/9/195},
