	Journal = {\apj},
	Keywords = {BINARY STARS, CARBON, DWARF STARS, NUCLEAR FUSION, STELLAR EVOLUTION, SUPERNOVAE, NUCLEAR FUSION, STELLAR MASS ACCRETION, STELLAR MODELS, STELLAR STRUCTURE, STELLAR TEMPERATURE},
	Month = oct,
	Pages = {531-537},
	Title = {{Carbon ignition in a rapidly accreting degenerate dwarf - A clue to the nature of the merging process in close binaries}},
