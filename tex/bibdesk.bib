	Author = {{Tewes}, M. and {Courbin}, F. and {Meylan}, G. and {Kochanek}, C.~S. and {Eulaers}, E. and {Cantale}, N. and {Mosquera}, A.~M. and {Magain}, P. and {Van Winckel}, H. and {Sluse}, D. and {Cataldi}, G. and {V{\"o}r{\"o}s}, D. and {Dye}, S.},
