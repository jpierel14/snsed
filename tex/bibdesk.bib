	Author = {{Rosati}, P. and {Balestra}, I. and {Grillo}, C. and {Mercurio}, A. and {Nonino}, M. and {Biviano}, A. and {Girardi}, M. and {Vanzella}, E. and {Clash-VLT Team}},
	Date-Added = {2016-01-15 15:17:36 +0000},
	Date-Modified = {2016-01-15 15:17:36 +0000},
	Journal = {The Messenger},
	Month = dec,
	Pages = {48-53},
	Title = {{CLASH-VLT: A VIMOS Large Programme to Map the Dark Matter Mass Distribution in Galaxy Clusters and Probe Distant Lensed Galaxies}},
