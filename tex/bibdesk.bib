	Author = {{Kriek}, M. and {Labb{\'e}}, I. and {Conroy}, C. and {Whitaker}, K.~E. and {van Dokkum}, P.~G. and {Brammer}, G.~B. and {Franx}, M. and {Illingworth}, G.~D. and {Marchesini}, D. and {Muzzin}, A. and {Quadri}, R.~F. and {Rudnick}, G.},
	Date-Added = {2014-07-31 11:32:47 +0000},
