	Doi = {10.1051/0004-6361:20053724},
	Eprint = {astro-ph/0606134},
	Journal = {\aap},
	Keywords = {Galaxy: formation, galaxies: high-redshift, galaxies: photometry, gravitational lensing},
	Month = sep,
	Pages = {861-880},
	Title = {{Constraining the population of 6 {\lap} $\{$z$\}$ {\lap} 10 star-forming galaxies with deep near-IR images of lensing clusters}},
	Volume = 456,
	Year = 2006,
