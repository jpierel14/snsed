	Doi = {10.1093/mnras/stu2430},
	Eprint = {1406.7431},
	Journal = {\mnras},
	Keywords = {binaries: general, stars: evolution, stars: winds, outflows},
	Month = feb,
	Pages = {598-617},
	Primaryclass = {astro-ph.SR},
	Title = {{Luminous blue variables are antisocial: their isolation implies that they are kicked mass gainers in binary evolution}},
	Volume = 447,
