	Archiveprefix = {arXiv},
	Arxivurl = {http://arxiv.org/abs/1212.2086},
	Author = {{Aulanier}, G. and {D{\'e}moulin}, P. and {Schrijver}, C.~J. and {Janvier}, M. and {Pariat}, E. and {Schmieder}, B.},
	Date-Added = {2016-06-02 11:38:47 +0000},
	Date-Modified = {2016-06-02 11:38:47 +0000},
	Doi = {10.1051/0004-6361/201220406},
	Eid = {A66},
	Eprint = {1212.2086},
	Journal = {\aap},
	Keywords = {magnetohydrodynamics (MHD), Sun: flares, solar-terrestrial relations, stars: flare},
