	Archiveprefix = {arXiv},
	Arxivurl = {http://arXiv.org/abs/0707.1891},
	Author = {{Holmberg}, J. and {Nordstr{\"o}m}, B. and {Andersen}, J.},
	Date-Added = {2014-09-18 16:40:03 +0000},
	Date-Modified = {2014-09-18 16:40:04 +0000},
	Doi = {10.1051/0004-6361:20077221},
	Eprint = {0707.1891},
