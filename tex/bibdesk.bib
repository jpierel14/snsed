	Date-Added = {2012-10-09 14:45:44 -0400},
	Date-Modified = {2012-10-09 14:45:45 -0400},
	Doi = {10.1111/j.1365-2966.2007.12038.x},
	Eprint = {arXiv:astro-ph/0609287},
