	Booktitle = {The 2002 HST Calibration Workshop},
	Date-Added = {2009-05-03 10:56:55 -1000},
	Date-Modified = {2013-11-08 17:00:23 -0500},
	Editor = {{Arribas}, S. and {Koekemoer}, A. and {Whitmore}, B.},
	Pages = {337},
	Read = {Yes},
	Title = {{MultiDrizzle: An Integrated Pyraf Script for Registering, Cleaning and Combining Images}},
