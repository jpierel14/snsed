	Adsurl = {http://adsabs.harvard.edu/abs/1995ApJ...455L.147N},
	Author = {{Nugent}, P. and {Phillips}, M. and {Baron}, E. and {Branch}, D. and {Hauschildt}, P.},
	Date-Added = {2009-06-22 10:49:22 -0700},
	Date-Modified = {2009-06-22 10:49:27 -0700},
	Doi = {10.1086/309846},
