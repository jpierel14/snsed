	Date-Added = {2009-11-16 21:42:26 -1000},
	Date-Modified = {2009-11-16 21:42:29 -1000},
	Doi = {10.1086/422207},
	Eprint = {arXiv:astro-ph/0309655},
	Journal = {\apj},
	Keywords = {Cosmology: Observations, Galaxies: Evolution, Galaxies: High-Redshift},
	Month = aug,
	Pages = {660-684},
	Title = {{Subaru Deep Survey. V. A Census of Lyman Break Galaxies at z\~{}=4 and 5 in the Subaru Deep Fields: Photometric Properties}},
