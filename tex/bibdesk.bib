	Journal = {\apj},
	Keywords = {galaxies: high-redshift, gravitational lensing: strong, gravitational lensing: weak, supernovae: general},
	Month = jul,
	Pages = {51},
	Title = {{Consistent Use of Type Ia Supernovae Highly Magnified by Galaxy Clusters to Constrain the Cosmological Parameters}},
