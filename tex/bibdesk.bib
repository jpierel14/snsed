	Author = {{Hern{\'a}n Caballero}, A.},
	Date-Added = {2014-08-04 18:55:36 +0000},
	Date-Modified = {2014-08-04 18:55:36 +0000},
	Doi = {10.1111/j.1365-2966.2012.22047.x},
	Eprint = {1209.0695},
