	Eprint = {astro-ph/0510256},
	Journal = {ArXiv Astrophysics e-prints},
	Keywords = {Astrophysics},
	Month = oct,
	Title = {{Modeling Supernova-like Explosions Associated with Gamma-ray Bursts with Short Durations}},
	Year = 2005,
