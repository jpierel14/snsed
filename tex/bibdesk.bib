	Eprint = {0801.0215},
	Journal = {\apj},
	Keywords = {galaxies: abundances, galaxies: evolution, supernovae: general},
	Month = dec,
	Pages = {1466-1484},
	Title = {{The Role of Type Ia Supernovae in Chemical Evolution. I. Lifetime of Type Ia Supernovae and Metallicity Effect}},
