	Eprint = {astro-ph/0307422},
	Journal = {\apj},
	Keywords = {Accretion, Accretion Disks, Cosmology: Dark Matter, Cosmology: Gravitational Lensing, Galaxies: Quasars: Individual: Alphanumeric: Q2237+0305, Stars: Luminosity Function, Mass Function},
