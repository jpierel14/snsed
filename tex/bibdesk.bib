	Journal = {\mnras},
	Keywords = {supernovae: general , cosmology: observations},
	Month = jun,
	Pages = {1893-1904},
	Title = {{Constraints on Type Ia supernova progenitor time delays from high-z supernovae and the star formation history}},
	Url = {http://adsabs.harvard.edu/abs/2006MNRAS.368.1893F},
	Volume = 368,
