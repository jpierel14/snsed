	Keywords = {GALAXIES: EVOLUTION, GALAXIES: PHOTOMETRY, COSMOLOGY: THEORY, GALAXIES: STELLAR CONTENT},
	Month = may,
	Pages = {106-+},
	Title = {{The Star Formation History of Field Galaxies}},
