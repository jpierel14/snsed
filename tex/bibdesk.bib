	Journal = {\mnras},
	Keywords = {supernovae: general, dust, extinction, galaxies: ISM},
	Month = sep,
	Pages = {671-680},
	Title = {{Extinction correction for Type Ia supernova rates - I. The model}},
	Url = {http://adsabs.harvard.edu/abs/2005MNRAS.362..671R},
