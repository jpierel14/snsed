	Date-Added = {2015-10-29 21:48:59 +0000},
	Date-Modified = {2015-10-29 21:49:00 +0000},
	Doi = {10.1051/0004-6361:20042422},
	Eprint = {astro-ph/0503019},
	Journal = {\aap},
	Keywords = {gravitational lensing, cosmological parameters, cosmology: observations},
	Month = jun,
	Pages = {25-35},
	Title = {{COSMOGRAIL: The COSmological MOnitoring of GRAvItational Lenses. I. How to sample the light curves of gravitationally lensed quasars to measure accurate time delays}},
