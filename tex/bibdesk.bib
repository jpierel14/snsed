	Doi = {10.1086/320958},
	Eprint = {astro-ph/0009101},
	Journal = {\apj},
	Keywords = {Cosmology: Observations, Galaxies: Clusters: General, Surveys, X-Rays: General},
	Month = jun,
	Pages = {668-676},
	Title = {{MACS: A Quest for the Most Massive Galaxy Clusters in the Universe}},
