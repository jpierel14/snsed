	Author = {{Botticella}, M.~T. and {Smartt}, S.~J. and {Kennicutt}, R.~C. and {Cappellaro}, E. and {Sereno}, M. and {Lee}, J.~C.},
	Date-Added = {2013-03-11 16:25:43 -0400},
	Date-Modified = {2013-03-11 16:25:43 -0400},
	Doi = {10.1051/0004-6361/201117343},
