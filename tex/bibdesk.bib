	Date-Added = {2015-04-05 06:38:17 +0000},
	Date-Modified = {2015-04-05 06:38:17 +0000},
	Doi = {10.1111/j.1365-2966.2009.14873.x},
	Eprint = {0904.1437},
	Journal = {\mnras},
	Keywords = {gravitational lensing , cosmological parameters},
	Month = jul,
	Pages = {311-319},
	Primaryclass = {astro-ph.CO},
