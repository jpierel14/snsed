	Journal = {\apj},
	Keywords = {galaxies: evolution, infrared: galaxies, radio continuum: galaxies},
	Month = jun,
	Pages = {1380-1397},
	Title = {{Balancing the Energy Budget Between Star Formation and Active Galactic Nuclei in High-Redshift Infrared Luminous Galaxies}},
	Volume = 698,
	Year = 2009,
