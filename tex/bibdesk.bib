	Journal = {Journal of the American Statistical Association},
	Number = {452},
	Pages = {1244},
	Read = {Yes},
	Title = {Inference for Deterministic Simulation Models: The Bayesian Melding Approach},
	Volume = {95},
	Year = {2000},
