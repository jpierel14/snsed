	Adsurl = {http://adsabs.harvard.edu/abs/2001IAUS..204..307S},
	Author = {{Savaglio}, S.},
	Booktitle = {The Extragalactic Infrared Background and its Cosmological Implications},
	Date-Added = {2009-11-23 14:07:31 -1000},
