	Author = {{Weinberg}, D.~H. and {Mortonson}, M.~J. and {Eisenstein}, D.~J. and {Hirata}, C. and {Riess}, A.~G. and {Rozo}, E.},
	Date-Added = {2016-10-06 19:40:21 +0000},
